{
    "Name": "Propositional Logic",
    "Symbols": [
        {
            "Type": "Negation",
            "Symbol": "~"
        },
        {
            "Type": "Conjunction",
            "Symbol": "&"
        },
        {
            "Type": "Left Parentheses",
            "Symbol": "("
        },
        {
            "Type": "Right Parentheses",
            "Symbol": ")"
        },
        {
            "Type": "Atomic Formula",
            "Symbol": ""
        }
    ],
    "Syntax": {
        "Context-free Grammar": {
            "Terminals": ["~", "&", "(", ")", "a"],
            "Non-Terminals": ["St", "Af"],
            "Production Rules": {
                "Ignore White Space": true,
                "Rules": [
                    "St -> ~St | ( St & St ) | Af",
                    "Af -> a"
                ]
            }
        }
    }
}
